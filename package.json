{
  "name": "mygithub",
  "version": "1.0.0",
  "description": "an app which lets user to access their GitHub repos and commits",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "web-bundle": "webpack --config app/web/config/webpack/prod.config.js --progress --colors",
    "web-dev-bundle": "webpack --config app/web/config/webpack/dev.config.js --progress --colors",
    "web-dev": "webpack-dev-server --content-base app/web/ --config app/web/config/webpack/dev.config.js --port 3002 --host 0.0.0.0 --inline --colors",
    "web-prod": "npm run web-bundle; node server/index.js"
  },
  "keywords": [
    "react",
    "redux",
    "recompose",
    "redux-thunk",
    "github"
  ],
  "author": "Anuraag Basu",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.27.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "node-sass": "^4.8.3",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  }
